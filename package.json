{
  "name": "ccc-react-paint",
  "version": "3.0.7",
  "keywords": [
    "ccc",
    "paint",
    "react"
  ],
  "description": "a simple paint tool for ccc fabric",
  "private": false,
  "main": "dist/ccc-paint-umd.min.js",
  "dependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "customize-cra": "^1.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start --hot",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "dist": "webpack --config webpack/webpack.dist.prod.config.js",
    "rollup-build": "rm -rf dist & cross-env NODE_ENV=production rollup -c"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "lint-staged": {
    "src/**/*.{jsx,tsx,ts,js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
